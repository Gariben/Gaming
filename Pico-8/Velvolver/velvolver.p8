pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
t=0
scn=2
blkw=8
blkh=8
-----begin core functions-----

function _init()
	game_init()
end

function game_init()
	
	s={h=100,s=100,b=6,r=false,rx=83}
	p={x=0,y=0,sp=0}
	
	--ui elements
	ui={by=20}
	
end


function _draw()
	game_draw()
end

function game_draw()
	cls()
	ui_draw()
end

function _update()
	t+=1
	game_update()
end

function game_update()
	--inputs
	
	if(btnp(5)) then
		roll()
	end
	
	if(btnp(4)) then
		--shots fired!
		gun()
	end
	
	
	
	if(s.r==true and s.rx<124) then
		s.rx+=1
	end
	
	--refresh values
	if(s.s<100) then
		s.s+=2
	end
	
	
	
end

----begin helper functions----

function ui_draw()
	--border
	rectfill(0,0,127,ui.by,7)

	--draw health frame
	rect(4,ui.by-10,31,ui.by-3,5)
	if(s.h>0) then
	rectfill(5,ui.by-9,5+(s.h/4),ui.by-4,8)
	end
	--draw stamina
	if(s.s>0) then
		rectfill(4,ui.by-2,4+(s.s*.4)-1,ui.by-1,3)
	end


	--draw bullet count
	
	if(s.b>0) then
		rectfill(120,ui.by-10,123,ui.by-3,8)
	end
	
	if(s.b>1) then
	rectfill(119,ui.by-10,120-((s.b-1)*4),ui.by-3,10)
	end
	
	--draw bullet frame
	for i=1,6 do
		spr(2,120-(i-1)*4,ui.by-10)
	end	

	--draw active reload bar
	rectfill(83,ui.by-2,123,ui.by-1,6)
	rectfill(100,ui.by-2,106,ui.by-1,5)

	--draw active reload
	if(s.r==true and s.rx<124) then
		line(s.rx,ui.by-4,s.rx,ui.by,10)
	end
	
end




function gun()
		
		if(s.r==false) then
			if(s.b>1) then
			--regular shot
				sfx(0,0,0)
				s.b=s.b-1
			elseif(s.b==1) then
			--power shot
				sfx(1,0,0)		
				s.b=s.b-1
		
			else
				sfx(2,0,0)
				s.rx=83
				s.r=true	
			--reload
			end
		else
			if(s.rx>=100 and s.rx<106) then
				sfx(3,0,0)
			else
				sfx(4,0,0)
			end
			s.b=6
			s.r=false
		end

end


function roll()
	if(s.s==100) then
		sfx(5,0,0)
		s.s=s.s-100
	end

end
