pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
t=0
scn=2
blkw=8
blkh=8
-----begin core functions-----

function _init()
	game_init()
end

function game_init()
	
	s={h=100,s=100,u=0,b=6,r=false,rx=83,rs=true}
	p={x=0,y=0,sp=0,r=false,s=false}
	
	--ui elements
	ui={by=20}
	
end


function _draw()
	game_draw()
end

function game_draw()
	cls()
	ui_draw()
end

function _update()
	t+=1
	game_update()
end

function game_update()
	--inputs
	
	if(btnp(5)) then
		roll()
	end
	
	if(btnp(4)) then
		--shots fired!
		gun()
	end
	
	
	
	if(s.r==true and s.rx<124) then
		s.rx+=2
	elseif(s.r==true and s.rx>=124) then
			sfx(4,0,0)
			s.rx=83
			s.b=6
			s.rs=false
			s.r=false
	end
	
	--refresh values
	if(s.s<100) then
		s.s+=2
	end
	
	
	
end

----begin helper functions----

function ui_draw()
	--border
	rectfill(0,0,127,ui.by,7)

	--draw health frame
	rect(4,ui.by-10,31,ui.by-3,5)
	if(s.h>0) then
		rectfill(5,ui.by-9,5+(s.h/4),ui.by-4,8)
	end
	
	--draw stamina
	if(s.s>0) then
		rectfill(4,ui.by-2,4+(s.s*.4)-1,ui.by-1,3)
	end

	--draw ult meter
	
	circfill(64,10,9,6)
	
	
	if(s.u>((1*100)/6)) then
		circfill(69,7,2,9)
	end

	if(s.u>((1*100)/3)) then
		circfill(69,13,2,9)
	end
	if(s.u>((1*100)/2)) then
		circfill(64,16,2,9)
	end
	if(s.u>((2*100)/3)) then
		circfill(59,13,2,9)
	end
	if(s.u>((5*100)/6)) then
			circfill(59,7,2,9)	
	end
	if(s.u==100) then
		circfill(64,4,2,9)	
	end

	
	
	
	for i=0,2*3.14*(s.u/100),(2*3.14/360) do
		cx=64 + 9*cos(i)
		cxd=cx%1
		if (cxd>0.5) then 
			cxd=1-cxd
			cx=cx+cxd 
		else
			cx=cx-cxd
		end
		cy=10 + 9*sin(i)
		cyd=cy%1
		if (cyd>0.5) then 
			cyd=1-cyd
			cy=cy+cyd 
		else
			cy=cy-cyd
		end		
		
		pset(cx,cy,5)
	end
	
	--draw bullet count
	
	if(s.b>0 and s.rs==true) then
		rectfill(120,ui.by-10,123,ui.by-3,8)
	elseif (s.b>0 and s.rs==false) then
		rectfill(120,ui.by-10,123,ui.by-3,10)
	end
	
	if(s.b>1) then
	rectfill(119,ui.by-10,120-((s.b-1)*4),ui.by-3,10)
	end
	
	if (s.b==0) then
		print("reload!",100,4,10)
	end
	
	--draw bullet frame
	for i=1,6 do
		spr(2,120-(i-1)*4,ui.by-10)
	end	

	--draw active reload bar
	rectfill(83,ui.by-2,123,ui.by-1,6)
	rectfill(100,ui.by-2,106,ui.by-1,5)

	--draw active reload
	if(s.r==true and s.rx<124) then
		line(s.rx,ui.by-4,s.rx,ui.by,10)
	end
	
	
	print(s.u,64,64,10)
end




function gun()
		
		if(s.r==false) then
			if(s.b>1) then
			--regular shot
				sfx(0,0,0)
				s.b=s.b-1
			elseif(s.b==1 and s.rs==true) then
			--power shot
				sfx(1,0,0)		
				s.b=s.b-1	
			elseif(s.b==1) then
			--regular shot
				sfx(0,0,0)
				s.b=s.b-1		
			else
				sfx(2,0,0)
				s.rx=83
				s.r=true	
			--reload
			end
		else
			if(s.rx>=100 and s.rx<106) then
				sfx(3,0,0)
				s.u+=5
				s.rs=true
			else
				sfx(4,0,0)
				s.rs=false
			end
			s.b=6
			s.r=false
		end

end


function roll()
	if(s.s==100) then
		sfx(5,0,0)
		s.s=s.s-98
	end

end
